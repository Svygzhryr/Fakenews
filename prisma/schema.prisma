generator client {
  provider = "prisma-client-js"
}

datasource db {
  // change provider to the preferred database and modify files accordingly
  provider = "sqlite"
  url      = "file:./dev.db"
}

model posts{
  id String @id @default(uuid())
  title String
  text String
  createdDate DateTime @default(now())
}